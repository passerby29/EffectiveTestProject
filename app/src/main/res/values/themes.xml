<resources>
    <!-- Base application theme. -->
    <style name="Theme.EffectiveTestProject" parent="Theme.Material3.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#3D3D3D</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="shape">
        <item name="cornerSize">50%</item>
    </style>

    <style name="activeIndicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">#EEEFF4</item>
    </style>

    <style name="et_style">
        <item name="android:textSize">18sp</item>
        <item name="fontFamily">@font/montserrat_medium</item>
        <item name="android:padding">6dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
    </style>
</resources>